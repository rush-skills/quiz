<p id="notice"><%= notice %></p>

<h1>Listing Test Attempts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Test</th>
      <th>Marks</th>
      <th>Time taken</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_attempts.each do |test_attempt| %>
      <tr>
        <td><%= test_attempt.user %></td>
        <td><%= test_attempt.test %></td>
        <td><%= test_attempt.marks %></td>
        <td><%= test_attempt.time_taken %></td>
        <td><%= link_to 'Show', test_attempt %></td>
        <td><%= link_to 'Edit', edit_test_attempt_path(test_attempt) %></td>
        <td><%= link_to 'Destroy', test_attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test attempt', new_test_attempt_path %>
