<div class="well pwell">
<h1>Listing Packages<span class="pull-right"><%= link_to new_package_path do %><button class="btn btn-fab btn-raised btn-success"><i class="mdi-content-add"></i></button><%end%></span></h1><br>


    <div class="row nolink">
    <% @packages.each do |package| %>
      <%=link_to package do %>
      <div class="col-md-3 col-sm-6 col-xs-12">
        <div class="well">
          <h5>
            <b>Name:</b><%= package.name %><br><br>
          </h5>
          <div class="img-qaa">
            <%= image_tag package.image if package.image %>
          </div>
        </div>
      </div>
      <%end%>
    <% end %>
    </div>

</div>

